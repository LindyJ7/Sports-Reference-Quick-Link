{
    "manifest_version": 2,
    "name": "BRef Quick Search",
    "version": "0.1",
    "content_scripts": [
      {
	    "matches": [
	      "<all_urls>"
	    ],
	    "js": ["sketch.js","content.js"]
      }
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": "ct.png",
      "default_popup": "index.html",
      "default_title": "Look up a player!"
    },
    "permissions": [
      "activeTab"
    ]
}